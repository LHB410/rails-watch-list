<div class="drk-bg">
  <div class="container">
    <h1 class="cursive text-center">Create a brand new list</h1>
    <div class="form-card-bg" style="background-image: url(http://source.unsplash.com/1600x900/?cinema&format=webp)">
      <div class="form-card">
        <%= simple_form_for @list do |f| %>
          <%= f.input :name %>
          <%= f.submit class: "btn btn-primary" %>
        <% end %>
        <%= link_to "Back", lists_path  %>
      </div>
    </div>
  </div>
</div>

